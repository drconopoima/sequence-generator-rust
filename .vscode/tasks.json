{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cargo",
      "subcommand": "check",
      "problemMatcher": ["$rustc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "label": "Rust: cargo check build - sequence-generator-rust"
    },
    {
      "type": "shell",
      "command": "cargo",
      "args": ["build", "--release"],
      "problemMatcher": ["$rustc"],
      "group": "none",
      "label": "Rust: cargo build release - sequence-generator-rust"
    },
    {
      "type": "shell",
      "command": "cargo",
      "args": ["fmt"],
      "problemMatcher": ["$rustc"],
      "group": "none",
      "label": "Rust: cargo fmt - sequence-generator-rust"
    },
    {
      "type": "cargo",
      "subcommand": "run",
      "problemMatcher": ["$rustc"],
      "group": "none",
      "label": "Rust: cargo run debug - sequence-generator-rust"
    },
    {
      "type": "shell",
      "command": "cargo",
      "args": ["test"],
      "problemMatcher": ["$rustc"],
      "group": {
        "kind": "test",
        "isDefault": true
      },
      "label": "Rust: cargo test - sequence-generator-rust"
    }
  ]
}
